{
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1.html",
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate"
        }
      },
      {
        "src": "/(.*)/",
        "dest": "/$1.html",
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate"
        }
      }
    ]
  }